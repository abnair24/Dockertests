<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GRID SAMPLE TEST" parallel="tests" thread-count="2">
    <test name="Chrome Tests" parallel="classes" thread-count="4">
        <parameter name="browserType" value="chrome"/>
        <classes>
            <class name="com.dockerSel.tests.SampleTest1"/>
            <class name="com.dockerSel.tests.SampleTest2"/>
        </classes>
    </test>
    <test name="Firefox Tests" parallel="classes" thread-count="4">
        <parameter name="browserType" value="firefox"/>
        <classes>
            <class name="com.dockerSel.tests.SampleTest1"/>
            <class name="com.dockerSel.tests.SampleTest2"/>
        </classes>
    </test>
</suite>